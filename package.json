{
  "name": "fun-apply",
  "version": "0.1.1",
  "description": "(f, [a, b, ...]) -> f(a, b, ...)",
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tap -Rtap --coverage-report=text-summary ./test/index.js",
    "coverage": "tap --coverage-report=lcov ./test/index.js",
    "predependencies": "mkdir -p img",
    "dependencies": "madge . --image img/dependencies.svg -x 'test|coverage|docs' --include-npm",
    "postdependencies": "npm run dependencies-test",
    "dependencies-test": "madge . --image img/dependencies-test.svg -x 'coverage|docs' --include-npm",
    "pages-index": "showdown makehtml -i README.md -o public/index.html",
    "pages": "./script/pages.sh",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern dist* --fix .",
    "predocument": "rm -rf docs",
    "document": "jsdoc -c .jsdoc.json",
    "readme": "cat readme/* > README.md",
    "preweb": "mkdir -p dist",
    "web": "browserify src/index.js -o dist/fun-apply-$(node -pe \"require('./package').version\").js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bagrounds/fun-apply.git"
  },
  "author": "bagrounds@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/bagrounds/fun-apply/issues"
  },
  "homepage": "https://gitlab.com/bagrounds/fun-apply#README",
  "devDependencies": {
    "eslint": "^3.18.0",
    "jsdoc": "^3.4.3",
    "madge": "^1.6.0",
    "showdown": "^1.6.4",
    "tap": "^10.3.1"
  },
  "dependencies": {
    "fun-assert": "^0.12.0",
    "guarded": "^0.5.1"
  }
}
